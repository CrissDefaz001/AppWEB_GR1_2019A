<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Probando Typescript</title>
    <link rel="stylesheet" type="text/css" href="../views/css/bootstrap.css">
</head>
<body>
    <div class="container">
        <h1>My favouite music</h1>
        <a href="/music/crear">
            <button>Add new</button>
        </a>
        <br/>
        <br/>
        <table class="table table-striped table-hover table-sm">
            <tr>
                <th>title</th>
                <th>album</th>
                <th>release</th>
                <th>artist</th>
                <th>trackNum</th>
                <th>genre</th>
                <th>Options</th>
            </tr>
                <% arrayMusic.forEach((music)=> { %>
            <tr>
                <td><%=music.title %></td>
                <td><%=music.album %></td>
                <td><%=music.release %></td>
                <td><%=music.artist %></td>
                <td><%=music.trackNum %></td>
                <td><%=music.genre %></td>
                <td>
                    <button>Editar</button>
                    <button>Eliminar</button>
                </td>
            </tr>
            <% }) %>
        </table>
    </div>
</body>
</html>
